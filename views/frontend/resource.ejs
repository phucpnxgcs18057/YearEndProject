<!DOCTYPE html>
<html>
  <head>
    <%- include("../partials/head.ejs") %>
  </head>

  <body>
    <div class="colorlib-loader"></div>

    <div id="page">
      <%- include("../partials/nav.ejs") %>

      <aside id="colorlib-hero">
        <div class="flexslider">
          <ul class="slides">
            <li style="background-image: url(images/img_bg_2.jpg)">
              <div class="overlay"></div>
              <div class="container-fluid">
                <div class="row">
                  <div
                    class="col-md-6 col-sm-12 col-md-offset-3 col-xs-12 slider-text"
                  >
                    <div class="slider-text-inner text-center">
                      <h1>Resource Detail</h1>
                    </div>
                  </div>
                </div>
              </div>
            </li>
          </ul>
        </div>
      </aside>

      <div class="colorlib-search">
        <div class="container">
          <div class="row">
            <div class="col-md-12 search-wrap">
              <!-- Search bar start -->
              <div class="search-wrap-2">
                <form method="post" class="colorlib-form">
                  <div class="row">
                    <div class="col-md-4">
                      <div class="form-group">
                        <!-- <label for="search">Search Course</label> -->
                        <div class="form-field">
                          <input
                            type="text"
                            id="search"
                            class="form-control"
                            placeholder="Resource Name"
                          />
                        </div>
                      </div>
                    </div>
                    <div class="col-md-4">
                      <div class="form-group">
                        <!-- <label for="search">Search Course</label> -->
                        <div class="form-field">
                          <input
                            type="text"
                            id="search"
                            class="form-control"
                            placeholder="Department"
                          />
                        </div>
                      </div>
                    </div>
                    <div class="col-md-3">
                      <input
                        type="submit"
                        name="submit"
                        id="submit"
                        value="Search"
                        class="btn btn-primary btn-block"
                      />
                    </div>
                  </div>
                </form>
              </div>
              <!-- Search bar end -->
            </div>
          </div>
        </div>
      </div>

      <div class="colorlib-classes">
        <div class="container">
          <div class="row">
            <div class="col-md-12 ml-auto mr-auto">
              <div class="row row-pb-lg">
                <div class="col-md-12 animate-box">
                  <div
                    class="classes class-single"
                  >
                    <div
                      class="classes-img"
                      style="background-image: url(images/classes-1.jpg)"
                    ></div>
                    <div class="desc desc2">
                      <h3>Resource Title</h3>
                      <p>
                       Content Here
                      </p>
                      <p>
                        <a href="#" class="btn btn-primary btn-outline btn-lg"
                          >Save to Library</a
                        >
                        or
                        <a href="#" class="btn btn-primary btn-lg"
                          >Remove from Library</a
                        >
                      </p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <%- include("../partials/footer.ejs") %>
    </div>

    <div class="gototop js-top">
      <a href="#" class="js-gotop"><i class="icon-arrow-up2"></i></a>
    </div>

    <!-- js start here -->

    <%- include("../partials/script.ejs") %>

    <!-- js end here -->
  </body>
</html>
